{
  "name": "backend",
  "version": "1.0.0",
  "description": "Quick REPC RESTful backend",
  "main": "server/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quick-REPC/backend.git"
  },
  "engines": {
    "node": "11"
  },
  "author": {
    "name": "Andrew Garvin",
    "email": "andrew@quickrepc.com",
    "url": "https://andrewgarv.in"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Quick-REPC/backend/issues"
  },
  "scripts": {
    "test": "dotenv -e .env.development jest",
    "start": "rm -rf ./node_modules/.cache/babel-loader && nodemon server/app.js --exec \"npx babel-node -r node_modules/dotenv/config\"",
    "start:prod": "babel-node server/app.js -r node_modules/dotenv/config",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:create": "db-migrate create",
    "lint": "eslint server",
    "format": "npm run prettier -- --write",
    "precommit": "lint-staged",
    "prettier": "prettier \"**/*.+(ts|js|jsx|json|yml|yaml|scss|css)\"",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@sentry/node": "^5.10.2",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validation": "^1.0.3",
    "joi": "^14.3.1",
    "joi-phone-number": "^3.1.1",
    "lodash": "^4.17.15",
    "massive": "^6.1.1",
    "moment": "^2.24.0",
    "pg": "^7.12.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "chalk": "^2.4.2",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "dotenv": "^8.1.0",
    "dotenv-cli": "^2.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-jest": "^22.17.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2"
  }
}
